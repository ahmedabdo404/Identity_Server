@model LoginViewModel

<form asp-controller="account"
      asp-action="Login"
    method="post">
    <input type="hidden" asp-for="ReturnUrl"/>
    <div>
        <label>UserName</label>
        <input asp-for="UserName" />
    </div>
    <div>
        <label>Password</label>
        <input asp-for="Password" />
    </div>
    <div>
        <button type="submit">SignIn</button>
    </div>
    

</form>
